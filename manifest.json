{
  "manifest_version": 3,
  "name": "Custom Shortcut Extension",
  "version": "1.0",
  "description": "Open custom URLs using shortcut keys",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "commands"],
  "host_permissions": ["<all_urls>"],
  "options_page": "options/options.html",
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Custom Shortcut"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "commands": {
    "open-custom-url": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "description": "Open Settings"
    },
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P"
      },
      "description": "Open Popup"
    },
    "shortcut-01": {
      "description": "Keyboard Shortcut 1"
    },
    "shortcut-02": {
      "description": "Keyboard Shortcut 2"
    },
    "shortcut-03": {
      "description": "Keyboard Shortcut 3"
    },
    "shortcut-04": {
      "description": "Keyboard Shortcut 4"
    },
    "shortcut-05": {
      "description": "Keyboard Shortcut 5"
    },
    "shortcut-06": {
      "description": "Keyboard Shortcut 6"
    },
    "shortcut-07": {
      "description": "Keyboard Shortcut 7"
    },
    "shortcut-08": {
      "description": "Keyboard Shortcut 8"
    },
    "shortcut-09": {
      "description": "Keyboard Shortcut 9"
    },
    "shortcut-10": {
      "description": "Keyboard Shortcut 10"
    }
  }
}